[
  {
    "kernelSource": {
      "name": "Oneplus7Pro",
      "repo": "https://github.com/charlessimmons334/nethunter_kernel_oneplus_sm8150.git",
      "branch": "nethunter-11.0",
      "device": "sm-8150",
      "defconfig": "oneplus7A11_defconfig"
    },
    "toolchains": [
      {
        "name": "arm64",
        "repo": "https://github.com/Dhruvgera/EvaGCC-arm64",
        "branch": "master",
        "binaryEnv": ["bin"]
      },
      {
        "name": "arm32",
        "repo": "https://github.com/Dhruvgera/EvaGCC-arm",
        "branch": "master",
        "binaryEnv": ["bin"]
      },
      {
        "name": "proton-clang",
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "binaryEnv": ["bin"]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "gcc",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-elf-",
        "CROSS_COMPILE_ARM32": "arm-eabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "LD": "aarch64-elf-ld.lld",
        "NM": "llvm-nm",
        "OBJCOPY": "llvm-objcopy"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
      },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
