[
  {
    "kernelSource": {
      "name": "gto",
      "repo": "https://github.com/charlessimmons334/android_kernel_samsung_gto_smt295.git",
      "branch": "lineageOS-17.1",
      "device": "SM-T295",
      "defconfig": "nethunter_defconfig"
    },
     "toolchains": [
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9",
        "branch": "android10-release",
        "name": "gcc-64"
        "binaryEnv": ["./bin"]
      },
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/arm/arm-linux-androideabi-4.9",
        "branch": "android10-release",
        "name": "gcc-32"
        "binaryEnv": ["./bin"]
      },
      {
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "name": "proton-clang",
        "binaryEnv": ["./bin"]
       } 
     ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "gcc",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-android-",
        "CROSS_COMPILE_ARM32": "arm-eabi-",
        "LD": "aarch64-linux-androidkernel-ld",
        "AR": "aarch64-linux-androidkernel-ar",
        "NM": "aarch64-linux-androidkernel-nm",
        "OBJCOPY": "aarch64-linux-androidkernel-objcopy",
        "OBJDUMP": "aarch64-linux-androidkernel-objdump",
        "READELF": "aarch64-linux-androidkernel-readelf",
        "OBJSIZE": "aarch64-linux-androidkernel-size",
        "STRIP": "aarch64-linux-androidkernel-strip"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
