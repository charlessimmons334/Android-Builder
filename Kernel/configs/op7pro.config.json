[
  {
    "kernelSource": {
      "name": "Oneplus7Pro",
      "repo": "https://github.com/charlessimmons334/nethunter_kernel_oneplus_sm8150.git",
      "branch": "nethunter-11.0",
      "device": "sm-8150",
      "defconfig": "kali_defconfig"
    },
    "toolchains": [
      {
        "repo": "https://github.com/mvaisakh/gcc-arm64",
        "branch": "gcc-new",
        "name": "gcc-arm64",
        "binaryEnv": [
          "./bin"
        ]
      },
      {
        "repo": "https://github.com/Dhruvgera/EvaGCC-arm",
        "branch": "master",
        "name": "EvaGCC-arm",
        "binaryEnv": [
          "./bin"
        ]
      },
      {
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "name": "proton-clang",
        "binaryEnv": [
          "./bin"
        ]
       } 
     ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "gcc",
      "externalCommands": {
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE": "gcc-arm64/bin/aarch64-elf-",
        "CROSS_COMPILE_ARM32": "EvaGCC-arm/bin/arm-eabi-",
        "LD": "gcc-arm64/bin/aarch64-elf-ld",
        "AR": "gcc-arm64/bin/aarch64-elf-gcc-ar",
        "NM": "gcc-arm64/bin/aarch64-elf-gcc-nm",
        "OBJCOPY": "gcc-arm64/bin/aarch64-elf-objcopy",
        "OBJDUMP": "gcc-arm64/bin/aarch64-elf-objdump",
        "READELF": "gcc-arm64/bin/aarch64-elf-readelf",
        "OBJSIZE": "gcc-arm64/bin/aarch64-elf-size",
        "STRIP": "gcc-arm64/bin/aarch64-elf-strip",
        "LDGOLD": "gcc-arm64/bin/aarch64-elf-ld.gold",
        "LLVM_AR": "gcc-arm64/bin/llvm-ar",
        "LLVM_DIS": "proton-clang/bin/llvm-dis"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
