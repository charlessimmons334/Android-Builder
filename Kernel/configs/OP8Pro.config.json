[
  {
    "kernelSource": {
      "name": "NetErnels",
      "repo": "https://github.com/NetErnels/android_kernel_oneplus_sm8250",
      "branch": "R",
      "device": "unified_op8",
      "defconfig": "neternels_defconfig"
    },
    "toolchains": [
      {
        "name": "Proton-Clang",
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "binaryEnv": [
          "CC=clang",
          "LLVM=1",
          "LLVM_IAS=1",
          "AR=llvm-ar",
          "LD=ld.lld",
          "OBJDUMP=llvm-objdump",
          "STRIP=llvm-strip"
        ]
      },
      {
        "name": "GCC-64bit",
        "repo": "https://github.com/mvaisakh/gcc-arm64/archive/1a4410a4cf49c78ab83197fdad1d2621760bdc73.zip",
        "binaryEnv": [
          "CROSS_COMPILE=aarch64-linux-gnu-"
        ]
      },
      {
        "name": "GCC-32bit",
        "repo": "https://github.com/mvaisakh/gcc-arm/archive/c8b46a6ab60d998b5efa1d5fb6aa34af35a95bad.zip",
        "binaryEnv": [
          "CROSS_COMPILE_ARM32=arm-linux-gnueabi-"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "CROSS_COMPILE_COMPAT": "",
        "CLANG_TRIPLE": ""
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "https://github.com/NetErnels/AnyKernel3.git",
        "branch": "op8"
      }
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
