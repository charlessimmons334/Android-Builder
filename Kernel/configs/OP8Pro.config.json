[
  {
    "kernelSource": {
      "name": "NetErnels",
      "repo": "https://github.com/NetErnels/android_kernel_oneplus_sm8250",
      "branch": "R",
      "device": "unified_op8",
      "defconfig": "neternels_defconfig"
    },
    "toolchains": [
      {
        "name": "Proton Clang",
        "repo": "https://github.com/kdrag0n/proton-clang",
        "branch": "master",
        "binaryEnv": [
          "CC=clang",
          "LLVM=1",
          "LLVM_IAS=1",
          "AR=llvm-ar",
          "LD=ld.lld",
          "OBJDUMP=llvm-objdump",
          "STRIP=llvm-strip"
        ]
      },
      {
        "name": "GCC aarch64/arm32 (optional fallback)",
        "url": "https://github.com/mvaisakh/gcc-arm64 & https://github.com/mvaisakh/gcc-arm",
        "binaryEnv": [
          "CROSS_COMPILE=aarch64-linux-gnu-",
          "CROSS_COMPILE_ARM32=arm-linux-gnueabi-"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "CROSS_COMPILE_COMPAT": "",
        "CLANG_TRIPLE": ""
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true,
      "custom": {
        "repo": "https://github.com/NetErnels/AnyKernel3.git",
        "branch": "op8"
      }
    },
    "enableKernelSU": false,
    "enableLXC": false
  }
]
