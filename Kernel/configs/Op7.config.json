[
  {
    "kernelSource": {
      "name": "OP7",
      "repo": "https://github.com/cyberknight777/nethunter_kernel_oneplus_sm8150.git",
      "branch": "nethunter-11.0",
      "device": "GM1917",
      "defconfig": "kali_defconfig"
    },
    "toolchains": [
      {
        "repo": "https://github.com/Sepatu-Bot/arm64-gcc.git",
        "branch": "gcc-master",
        "name": "gcc-arm64",
        "binaryEnv": [
          "./bin"
        ]
      },
      {
        "repo": "https://github.com/Sepatu-Bot/gcc-arm32.git",
        "branch": "gcc-master",
        "name": "gcc-arm32",
        "binaryEnv": [
          "./bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "gcc",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-elf-",
        "CROSS_COMPILE_ARM32": "arm-eabi-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
