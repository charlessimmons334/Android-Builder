[
  {
    "kernelSource": {
      "name": "OP7",
      "repo": "https://github.com/cyberknight777/nethunter_kernel_oneplus_sm8150.git",
      "branch": "nethunter-11.0",
      "device": "GM1917",
      "defconfig": "kali_defconfig"
    },
    "toolchains": [
      {
        "repo": "https://github.com/theradcolor/aarch64-linux-gnu",
        "branch": "master",
        "name": "aarch64-linux-gnu",
        "binaryEnv": [
          "./bin"
        ]
      },
      {
        "repo": "https://github.com/theradcolor/arm-linux-gnueabi",
        "branch": "master",
        "name": "https://github.com/theradcolor/arm-linux-gnueabi",
        "binaryEnv": [
          "./bin"
        ]
      }
    ],
    "enableCcache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "gcc",
      "externalCommands": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-androideabi-"
      }
    },
    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
